      </physvol>
      <physvol copynumber="7" name="LDMXTaggerModuleVolume_physvol13">
        <volumeref ref="LDMXTaggerModuleVolume" />
        <position name="LDMXTaggerModuleVolume_physvol13_pos" unit="mm" x="-0.065" y="0" z="289.5" />
      </physvol>
      <physvol copynumber="7" name="LDMXTaggerModuleVolume_physvol14">
