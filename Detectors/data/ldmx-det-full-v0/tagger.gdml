      <solidref ref="tagger_box"/>
      <physvol copynumber="1" name="LDMXTaggerModuleVolume_physvol1">
        <volumeref ref="LDMXTaggerModuleVolume" />
        <position name="LDMXTaggerModuleVolume_physvol1_pos" unit="mm" x="-20.874" y="0" z="-310.5" />
